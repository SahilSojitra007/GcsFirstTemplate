<input type="button" value="Back" style="margin-top:10px; margin-left:15px" class="btn btn-outline-primary" onclick="location.href='@Url.Action("mywallet", "home")'" />

<div class="container-fluid">
    <div class="alert alert-dark" role="alert" style="margin-top:20px">
        <h1 align="center">Quotation Entry Detail</h1>
    </div>
</div>
<hr />

<div class="container">
    <form name="myform" class="form-group">
        <div class="row">
            <div class="col-sm-6 ">
                <div class="form-group">
                    <label>Date:</label>
                    <input type="date" class="form-control " id="exampleInputdate">
                </div>
            </div>
            <div class="col-sm-3">
                <label>Product Name:</label>
                <select class="form-control">
                    <option>Revise Report(210900384TPE-001)</option>
                    <option>Indoor LED Fixture Lamp</option>
                    <option>室内DC LED 固定式灯具*2+室内DC LED 移动式灯具*7</option>
                </select>

            </div>
            <div class="col-sm-1" style="margin-top:31px">
                <input type="number" class="form-control" />
            </div>
            <div class="col-sm-2" style="margin-top:31px">
                <select class="form-control">
                    <option>English</option>
                    <option>Simplified Chinese</option>
                    <option>Traditional Chinese</option>
                </select>
            </div>
            <div class="col-sm-3">
                <label>Certificate Agency Fees:</label>
                <input type="text" class="form-control" />
            </div>
            <div class="col-sm-2">
                <label>Currency:</label>
                <div class="currency">
                    <select class="form-control">
                        <option>RMB</option>
                        <option>NTD</option>
                        <option>EUR</option>
                        <option>HKD</option>
                        <option>INR</option>
                        <option>JPY</option>
                        <option>THB</option>
                        <option>USD</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-1" style="margin-top:31px">
                <input type="text" class="form-control" />
            </div>

            <div class="col-sm-4">
                <label>Model No:</label>
                <input type="number" name="project" class="form-control" />
            </div>
            <div class="col-sm-2" style="margin-top:31px">
                <select class="form-control">
                    <option>YES</option>
                    <option>NO</option>
                </select>
            </div>
            <div class="col-sm-5">
                <label>Attention:</label>
                <select class="form-control">
                    <option>charles vigna</option>
                    <option>marry rose</option>
                    <option>elvin xeiong</option>
                </select>
            </div>
            <div class="col-sm-1" style="margin-top:31px">
                <input type="text" class="form-control" />
            </div>
            <div class="col-sm-4">
                <label>Report No:</label>
                <input type="number" class="form-control" />
            </div>
            <div class="col-sm-2" style="margin-top:31px">
                <select class="form-control">
                    <option>YES</option>
                    <option>NO</option>
                </select>
            </div>
            <div class="col-sm-4">
                <label>Quotation No:</label>
                <input type="number" class="form-control" />
            </div>
            <div class="col-sm-2" style="margin-top:32px">
                <input type="button" style="width:160px" value="Get" class="btn btn-outline-primary" />
            </div>
            <div class="col-sm-6">
                <label>Remark:</label>
                <textarea class="form-control" id="floatingTextarea"></textarea>
            </div>

            <div class="col-sm-6">
                <label>Ammount:</label>
                <input type="text" class="form-control" />
            </div>
        </div>
    </form>
</div>
<hr />

